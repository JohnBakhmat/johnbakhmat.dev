{
    "headers": [
      {
        "source": "/:all*(ttf|otf|woff|woff2)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/github",
        "destination": "https://github.com/johnbakhmat"
      },
      {
        "source": "/gh",
        "destination": "https://github.com/johnbakhmat"
      },
      {
        "source": "/github/:slug*",
        "destination": "https://github.com/johnbakhmat/:slug*"
      },
      {
        "source": "/gh/:slug*",
        "destination": "https://github.com/johnbakhmat/:slug*"
      }
    ]
  }